{
  "name": "vue-financial-charts",
  "version": "1.0.0",
  "description": "A Vue 3 + Shadcn UI + D3.js financial chart component library",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:lib": "vue-tsc --emitDeclarationOnly && vite build --mode lib",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "keywords": [
    "vue",
    "vue3",
    "d3",
    "charts",
    "financial",
    "investment",
    "shadcn",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.0",
    "d3": "^7.8.5"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.0",
    "typescript": "~5.2.0",
    "vite": "^5.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0"
  },
  "dependencies": {
    "d3": "^7.8.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-vue-next": "^0.294.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7"
  }
